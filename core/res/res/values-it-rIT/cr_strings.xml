<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Button that copies the url to the log of a crashed application -->
    <string name="aerr_copy">Copia l\'URL del log di crash</string>
    <string name="url_copy_success">URL copiato con successo</string>
    <string name="url_copy_failed">Si è verificato un errore durante il caricamento del log su pasty</string>
    <!-- Lock gesture -->
    <string name="gesture_locked_warning">Scorri di nuovo per sbloccare i gesti</string>
    <!-- Pocket Lock -->
    <string name="pocket_mode_title">La modalità tasca è attiva</string>
    <string name="pocket_mode_illustration_description">Illustrazione del telefono in tasca</string>
    <string name="pocket_mode_instructions_header">Per utilizzare il telefono:</string>
    <string name="pocket_mode_instruction_step1">1. Assicurarsi che l\'area blu (sensore di prossimità) mostrata sopra sia ostruita e libera da sporcizia o polvere.</string>
    <string name="pocket_mode_instruction_step2">2. Premere e tenere premuto il pulsante di accensione per uscire dalla modalità tasca.</string>
    <!-- App lock -->
    <string name="unlock_application">Sblocca <xliff:g id="label" example="Telegram">%1$s</xliff:g></string>
