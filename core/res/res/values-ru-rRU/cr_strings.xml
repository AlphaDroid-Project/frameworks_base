<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Button that copies the url to the log of a crashed application -->
    <string name="aerr_copy">Скопировать ссылку журнала сбоев</string>
    <string name="url_copy_success">Ссылка успешно скопирована</string>
    <string name="url_copy_failed">Произошла ошибка во время загрузки лога на Pasty</string>
    <!-- Lock gesture -->
    <string name="gesture_locked_warning">Проведите ещё раз, чтобы разблокировать жесты</string>
    <!-- Pocket Lock -->
    <string name="pocket_mode_title">Режим «В кармане» включен</string>
    <string name="pocket_mode_illustration_description">Иллюстрация телефона в кармане</string>
    <string name="pocket_mode_instructions_header">Для использования телефона:</string>
    <string name="pocket_mode_instruction_step1">1. Убедитесь, что синяя область (датчик приближения), показанная выше, не закрыт и не содержит грязи или пыли.</string>
    <string name="pocket_mode_instruction_step2">2. Нажмите и удерживайте кнопку питания, чтобы выйти из режима «В кармане».</string>
    <!-- App lock -->
    <string name="unlock_application">Разблокировать <xliff:g id="label" example="Telegram">%1$s</xliff:g></string>
